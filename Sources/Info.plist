	<string>App requires access to your camera to scan credit cards</string>
	<key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
	<string>App requires access to your location always and when being used.</string>
	<key>NSLocationAlwaysUsageDescription</key>
	<string>App requires access to your location always</string>
	<key>NSLocationUsageDescription</key>
	<string>App requires access to your location</string>
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>App requires access to your location only when being used.</string>
	<key>NSPhotoLibraryUsageDescription</key>
	<string>App requires access to your photo library to use images</string>
	<key>UIBackgroundModes</key>
	<array>
		<string>remote-notification</string>
	</array>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>UIRequiredDeviceCapabilities</key>
	<array>
		<string>armv7</string>
	</array>
	<key>UIRequiresFullScreen</key>
	<true/>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
	</array>
</dict>
</plist>
